{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "incremental": true
  },
  "exclude": [
    "node_modules",
    ".expo",
    "dist",
    "build",
    "**/*.example.ts",
    "**/*.example.tsx"
  ]
}
